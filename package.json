{
  "name": "tunecamp",
  "version": "1.1.3",
  "description": "A modern static site generator for musicians and music labels, inspired by Faircamp",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "tunecamp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tunecamp": "node dist/cli.js",
    "gleam:build": "gleam build --target javascript && npx esbuild \"build/dev/javascript/tunecamp_gleam/gleam/audio_utils.mjs\" --bundle --outfile=\"src/gleam_generated/audio_utils.js\" --format=esm"
  },
  "keywords": [
    "music",
    "static-site-generator",
    "bandcamp",
    "artist",
    "album",
    "audio"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@types/archiver": "^7.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/inquirer": "^9.0.9",
    "@types/multer": "^2.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gun": "^0.2020.1241",
    "handlebars": "^4.7.8",
    "inquirer": "^13.2.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "music-metadata": "^8.1.4",
    "node-fetch": "^3.3.2",
    "node-id3": "^0.2.9",
    "vercel": "^50.4.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.13",
    "typescript": "^5.3.3"
  }
}
