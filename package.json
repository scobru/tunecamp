{
  "name": "tunecamp",
  "version": "2.0.0",
  "description": "A modern static site generator for musicians and music labels, inspired by Faircamp",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "tunecamp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & npm run watch:css",
    "start": "node dist/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tunecamp": "node dist/cli.js",
    "build:css": "tailwindcss -i webapp/css/input.css -o webapp/css/style.css",
    "watch:css": "tailwindcss -i webapp/css/input.css -o webapp/css/style.css --watch"
  },
  "keywords": [
    "music",
    "static-site-generator",
    "bandcamp",
    "artist",
    "album",
    "audio"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fedify/express": "^1.10.3",
    "@fedify/fedify": "^1.10.3",
    "@inquirer/prompts": "^8.2.0",
    "@js-temporal/polyfill": "^0.5.1",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/inquirer": "^9.0.9",
    "@types/md5": "^2.3.6",
    "@types/multer": "^2.0.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "gun": "^0.2020.1241",
    "handlebars": "^4.7.8",
    "inquirer": "^13.2.0",
    "jsonwebtoken": "^9.0.3",
    "md5": "^2.3.0",
    "multer": "^2.0.2",
    "music-metadata": "^8.1.4",
    "node-fetch": "^3.3.2",
    "node-id3": "^0.2.9",
    "ts-pattern": "^5.9.0",
    "vercel": "^50.4.4",
    "xmlbuilder2": "^4.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.13",
    "daisyui": "^5.5.16",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3"
  }
}
